<% provide(:title, t('ManagingUsers')) %>

<% provide :page_heading do %>
  <%= @user.user_name %>
<% end %>

<% provide :breadcrumbs do %>
  <ol id="breadcrumbs" class="breadcrumbs">
    <li>
      <a href="/" title="<%= t("Display") %>" class="fa fa-home"></a>
    </li>
    <li>
      <%= link_to @user.name %>
    </li>
  </ol>
<% end %>

<% provide :navigation do %>
  <%= render partial: "shared/edit_view_navigation" %>
<% end %>

<% provide :actions do %>
  <%= render partial: "shared/object_edit_actions", locals: {this_object: @user } %>
<% end %>

<%= render 'form' %>
