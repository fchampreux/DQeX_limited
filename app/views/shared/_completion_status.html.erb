  <div class="row mat-form-row">
    <% if @completion_status[0] %>
      <div class="mat-form-field col-md-2" title="<%= t('Translations_hint') %>" >
        <label for="translation-completion" class="mat-form-field-label">
          <%= t('Translations') %>
        </label>
        <input id="translation-completion" class="mat-input-element" readonly
          value="<%= number_to_percentage(@completion_status[0], precision: 0) %>" />
      </div>
    <% end %>
    <% if @completion_status[1] %>
      <div class="mat-form-field col-md-2" title="<%= t('ValuesList_hint') %>" >
        <label for="values_list-reference" class="mat-form-field-label">
