        </label>
        <input id="linked-variables" class="mat-input-element" readonly
          value="<%= number_to_percentage(@completion_status[2], precision: 0) %>" />
      </div>
    <% end %>
    <% if @completion_status[3] %>
      <div class="mat-form-field col-md-2" title="<%= t('Responsibilities_hint') %>" >
        <label for="responsibilities" class="mat-form-field-label">
          <%= t('Responsibilities') %>
        </label>
        <input id="responsibilities" class="mat-input-element" readonly
          value="<%= number_to_percentage(@completion_status[3], precision: 0) %>" />
      </div>
    <% end %>
    <% if @completion_status[4] %>
