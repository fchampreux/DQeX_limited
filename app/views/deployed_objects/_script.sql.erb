<p> create table <%= @business_object.code %> ( </p>
<table>
  <% @business_object.deployed_skills.visible.order(:sort_code).each do |skill| %>
    <tr>
      <td style="width:20%; font-family: monospace; text-align: left;"> <%= skill.code %> </td>
      <td style="width:20%; font-family: monospace; text-align: left;"> <%= t(skill.skill_type.code).downcase %>(<%= skill.skill_size %>),</td>
    </tr>
  <% end %>
</table >
);
