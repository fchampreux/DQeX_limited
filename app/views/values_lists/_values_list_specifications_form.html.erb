  <div class="row mat-form-row">
    <div class="mat-form-field col-md-12">
      <%= f.label :resource_name, t('resource'), class: "mat-form-field-label" %>
      <%= f.text_field :resource_name, class: "mat-input-element" %>
    </div>
  </div>
  <div class="row mat-form-row">
    <div class="mat-form-field col-md-4">
      <%= f.label :list_type, t('ValuesType'), class: "mat-form-field-label" %>
      <%= f.collection_select :list_type_id, values_list_types, :id, :property, {}, { class: "mat-input-element" }%>
    </div>
    <div class="mat-form-field col-md-4">
      <%= f.label :list_data_type, t('DataType'), class: "mat-form-field-label" %>
      <%= f.collection_select :code_type_id, options_for('data_types', 'ValuesList'), :id, :code, {}, { class: "mat-input-element" }%>
    </div>
    <div class="mat-form-field col-md-4">
      <%= f.label :code_max_length, t('CodeMaxLength'), class: "mat-form-field-label" %>
      <%= f.number_field :code_max_length, in: 1..32, step: 1, class: "mat-input-element" %>
    </div>
  </div>
