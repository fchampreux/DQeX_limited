<%# List events within a group of a production event and allow the user to see the details %>
<table id="production-events-list" class="table table-hover table-clickable" style="width:100%">
  <thead>
    <tr>
      <th> <%= t("scheduler.production_events.index.Status") %> </th>
      <th> <%= t("scheduler.production_events.index.Code") %> </th>
      <th> <%= t("scheduler.production_events.index.Technology") %> </th>
      <th> <%= t("scheduler.production_events.index.TargetObject") %> </th>
      <th> <%= t("scheduler.production_events.index.ReturnValue") %> </th>
      <th> <%= t("scheduler.production_events.index.SuccessNext") %> </th>
      <th> <%= t("scheduler.production_events.index.FailureNext") %> </th>
    </tr>
  </thead>

  <tbody>
    <% this_object.order(:success_next_id).each do |event| %>
      <tr>
        <td> <%= event.status&.code %> </td>
        <td> <%= event.task.code %> </td>
        <td> <%= event.technology&.code %> </td>
        <td> <%= event.target_object&.code %> </td>
        <td> <%= event.return_value %> </td>
        <td> <%= event.success_next&.task&.code %> </td>
        <td> <%= event.failure_next&.task&.code %> </td>
      </tr>
    <% end %>
  </tbody>

</table>



